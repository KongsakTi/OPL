val s_18 = "(x^2 + 7)^2 * (x + 1) * (3 * x^4 - 2)^2"
val e_18 = Parser(s_18).get
val p_18 = Process.differentiate(e_18, "x")
val x_18 = 7
val v_18 = Process.eval(p_18, Map("x" -> x_18))
val r_18 = "too lazy to type"
val a_18 = 2210

println(s_18, x_18)
println(r_18, v_18)

